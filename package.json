{
  "name": "vscode-quick-terminals",
  "displayName": "Quick Terminals",
  "description": "Open multiple terminals at the same time.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.68.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:vscode-quick-terminals.open-terminal",
    "onCommand:vscode-quick-terminals.open-terminals"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-quick-terminals.open-terminal",
        "title": "Open Terminal",
        "category": "Quick Terminals"
      },
      {
        "command": "vscode-quick-terminals.open-terminals",
        "title": "Open Terminals",
        "category": "Quick Terminals"
      }
    ]
  },
  "scripts": {
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.68.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0"
  }
}
