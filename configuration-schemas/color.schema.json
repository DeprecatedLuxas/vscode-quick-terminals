{
  "$schema": "http://json-schema.org/draft-07/schema",
  "template": true,
  "properties": {
    "color": {
      "type": "string",
      "description": "The color of the terminal.",
      "enum": [
        "black",
        "red",
        "green",
        "yellow",
        "blue",
        "magenta",
        "cyan",
        "white"
      ],
      "default": "white"
    }
  }
}
